<?xml version="1.0" ?>
<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="jphooiveld_eventsauce.message_repository.doctrine" class="EventSauce\MessageRepository\DoctrineMessageRepository\DoctrineUuidV4MessageRepository" public="false">
            <argument/>
            <argument>%jphooiveld_eventsauce.repository.doctrine.table%</argument>
            <argument type="service" id="jphooiveld_eventsauce.message_serializer"/>
            <argument>%jphooiveld_eventsauce.repository.doctrine.json_encode_options%</argument>
        </service>
    </services>
</container>
